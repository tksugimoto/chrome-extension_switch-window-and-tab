{
	"name": "Switch tab/window",
	"version": "0.1",
	"manifest_version": 3,

	"description": "タブ・ウインドウを一覧表示・切り替え",

	"permissions": [
		"bookmarks",
		"tabs"
	],
	"host_permissions": [
		"<all_urls>"
	],

	"background": {
		"service_worker": "background.js"
	},

	"action": {

	},

	"commands": {
		"switch_tab": {
			"description": "タブの切り替えウィンドウ表示",
			"suggested_key": {
				"default": "Ctrl+Space"
			}
		}
	}
}
