{
	"name": "Switch tab/window",
	"version": "0.1",
	"manifest_version": 2,

	"description": "タブ・ウインドウを一覧表示・切り替え",

	"permissions": [
		"bookmarks",
		"tabs",
		"<all_urls>"
	],

	"background": {
		"scripts": [
			"background.js"
		]
	},

	"browser_action": {
		
	},

	"commands": {
		"switch_tab": {
			"description": "タブの切り替えウィンドウ表示",
			"suggested_key": {
				"default": "Ctrl+Space"
			}
		}
	}
}
